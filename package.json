{
  "name": "kanizsa-mcp-photo-server",
  "version": "7.0.1",
  "description": "Kanizsa MCP server for photo analysis with adjective agent integration",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "mcp-photo-server": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "node test-improvements.js",
    "test:type-safety": "node -e \"import('./src/shared-testing.js').then(m => m.TypeSafetyTester.testSharedTypeCompatibility()).then(console.log)\"",
    "test:integration": "node -e \"import('./src/shared-testing.js').then(m => m.IntegrationTester.testCacheIntegration()).then(console.log)\""
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "zod": "^3.22.4",
    "node-fetch": "^3.3.2",
    "redis": "^4.6.12",
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  }
}