{
  "name": "kanizsa-mcp-photo-server",
  "version": "11.2.2",
  "description": "Kanizsa MCP server for photo analysis with agent integration",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "mcp-photo-server": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "node test-improvements.js",
    "test:type-safety": "node -e \"import('./src/shared-testing.js').then(m => m.TypeSafetyTester.testSharedTypeCompatibility()).then(console.log)\"",
    "test:integration": "node -e \"import('./src/shared-testing.js').then(m => m.IntegrationTester.testCacheIntegration()).then(console.log)\""
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.2",
    "zod": "^4.0.15",
    "node-fetch": "^3.3.2",
    "redis": "^5.8.0",
    "express": "^5.1.0",
    "cors": "^2.8.5",
    "helmet": "^8.1.0",
    "express-rate-limit": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "tsx": "^4.6.2",
    "typescript": "^5.2.2"
  }
}